{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    { "token": "", "foreground": "#ffffff", "background": "#000000" },
    { "token": "keyword", "foreground": "#ff7b72" },
    { "token": "keyword.operator", "foreground": "#ff7b72" },
    { "token": "keyword.control", "foreground": "#ff7b72" },
    { "token": "number", "foreground": "#79c0ff" },
    { "token": "string", "foreground": "#a5d6ff" },
    { "token": "comment", "foreground": "#8b949e" },
    { "token": "delimiter", "foreground": "#8b949e" },
    { "token": "tag", "foreground": "#79c0ff" },
    { "token": "attribute.name", "foreground": "#79c0ff" },
    { "token": "attribute.value", "foreground": "#79c0ff" },
    { "token": "attribute.value.number", "foreground": "#79c0ff" },
    { "token": "attribute.value.unit", "foreground": "#79c0ff" },
    { "token": "attribute.value.hex", "foreground": "#79c0ff" },
    { "token": "attribute.value.rgba", "foreground": "#79c0ff" },
    { "token": "attribute.value.rgb", "foreground": "#79c0ff" },
    { "token": "attribute.value.unicode-range", "foreground": "#79c0ff" },
    { "token": "attribute.value.url", "foreground": "#79c0ff" },
    { "token": "attribute.value.function", "foreground": "#79c0ff" },
    { "token": "meta", "foreground": "#79c0ff" },
    { "token": "type", "foreground": "#79c0ff" },
    { "token": "variable", "foreground": "#79c0ff" },
    { "token": "variable.parameter", "foreground": "#79c0ff" },
    { "token": "variable.language", "foreground": "#ff7b72" },
    { "token": "function", "foreground": "#d2a8ff" },
    { "token": "class", "foreground": "#d2a8ff" },
    { "token": "class.inherited", "foreground": "#d2a8ff" },
    { "token": "class.inherited.protected", "foreground": "#d2a8ff" },
    { "token": "tag", "foreground": "#7ee787" },
    { "token": "tag.css", "foreground": "#ffffff" },
    { "token": "emphasis", "fontStyle": "italic" },
    { "token": "strong", "fontStyle": "bold" },
    { "token": "addition", "foreground": "#aff5b4", "background": "#033a16" },
    { "token": "deletion", "foreground": "#ffdcd7", "background": "#67060c" },
    { "token": "metatag.html", "foreground": "#79c0ff" },
    { "token": "delimiter.html", "foreground": "#ffffff" },
    { "token": "attribute.value.number.css", "foreground": "#ffffff" },
    { "token": "attribute.value.unit.css", "foreground": "#ffffff" },
    { "token": "delimiter.css", "foreground": "#ffffff" },
    { "token": "attribute.value.html", "foreground": "#a5d6ff" },
    { "token": "attribute.value.css", "foreground": "#ffffff" },
    { "token": "metatag.content.html", "foreground": "#ff7b72" },
    { "token": "attribute.value.hex.css", "foreground": "#ffffff" },
    { "token": "number.js", "foreground": "#ffffff" }
  ],
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#ffffff",
    "editorLineNumber.foreground": "#8b949e",
    "editorLineNumber.activeForeground": "#ffffff",
    "editorCursor.foreground": "#ffffff",
    "editor.selectionBackground": "#1f6feb",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#1f6feb50",
    "editor.wordHighlightBackground": "#1f6feb50",
    "editor.wordHighlightStrongBackground": "#1f6feb50",
    "editor.findMatchBackground": "#1f6feb",
    "editor.findMatchHighlightBackground": "#1f6feb50",
    "editor.findRangeHighlightBackground": "#1f6feb50",
    "editor.hoverHighlightBackground": "#1f6feb50",
    "editor.lineHighlightBackground": "#1f6feb20",
    "editor.lineHighlightBorder": "#1f6feb50",
    "editorLink.activeForeground": "#79c0ff",
    "editorWhitespace.foreground": "#ffffff"
  }
}